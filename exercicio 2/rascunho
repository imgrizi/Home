<body>
    <script>
        calçculaMedia();
        
        function calculaMedia(){
        var lista=[7,5,9,66,9,5,36,11];
        soma=0;
        for(i=0; i<=7; i=i+1){
            lido=lista[i];
            soma=soma+lido;
        }
        
        media=soma/7;
        document.write("Média= "+media);
    }

    </script>

</body>
